<?xml version="1.0" encoding="utf-8" ?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2" >
  <Program name="usrSelect" access="public" >
    <Parameters xmlns="http://www.staubli.com/robotics/VAL3/Param/1" >
      <Parameter name="nWhichBttn" type="num" use="reference" xsi:type="element" dimensions="1" />
    </Parameters>
    <Code><![CDATA[begin
  nWhichBttn=-1
  do
    nWhichBttn=(get()-270)
  until ((nWhichBttn==1) or (nWhichBttn==8))
end]]></Code>
  </Program>
</Programs>